{
  "name": "jaime-ci-bot",
  "version": "1.0.0",
  "private": true,
  "description": "A CI Bot based on Probot to aid Pull Requests on the SDK Repository",
  "author": "MeFisto94 <MeFisto94@users.noreply.github.com>",
  "license": "ISC",
  "repository": "https://github.com/MeFisto94/jaime-ci-bot.git",
  "homepage": "https://github.com/MeFisto94/jaime-ci-bot",
  "bugs": "https://github.com/MeFisto94/jaime-ci-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "bull": "^3.12.1",
    "child-process-promise": "^2.2.1",
    "diff": "^4.0.1",
    "git-diff": "^2.0.6",
    "p-queue": "^6.2.1",
    "probot": "^9.5.3",
    "workerpool": "^5.0.3",
    "xmldom": "^0.2.1",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^2.0.2",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
